    "url": "https://github.com/LouisVelz/groupride/issues"
  },
  "homepage": "https://github.com/LouisVelz/groupride#readme",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@react-google-maps/api": "^1.9.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "loadscript": "0.0.1",
    "mongoose": "^5.9.18",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
